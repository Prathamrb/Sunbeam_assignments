package Que1;
class Rectangle
{private int l=20;
private int b=10;
/*Rectangle(int l, int b)
{
	this.l=l;
	this.b=b;
}*/
	public int area()
	{
		return (l*b);
	}
}
public class que1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Rectangle r=new Rectangle();
		int rs= r.area();
		System.out.print(rs);
	}

}
